{
  "defaultSeverity": "error",
  "extends": ["tslint:latest"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "arrow-parens": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "no-console": false,
    "no-any": true,
    "no-implicit-dependencies": false,
    "interface-name": false
  },
  "rulesDirectory": []
}
